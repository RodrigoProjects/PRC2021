@prefix : <http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5> .

<http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5> rdf:type owl:Ontology ;
                                                            owl:versionIRI <http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5/1.0.0> ;
                                                            rdfs:label "Ontologia do TPC5 - Bibliografia" .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5#hasAuthor
:hasAuthor rdf:type owl:ObjectProperty ;
           owl:inverseOf :isAuthor ;
           rdfs:domain :Resource ;
           rdfs:range :Author .


###  http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5#hasEditor
:hasEditor rdf:type owl:ObjectProperty ;
           owl:inverseOf :isEditor ;
           rdfs:domain :Resource ;
           rdfs:range :Author ,
                      :Editor .


###  http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5#isAuthor
:isAuthor rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5#isEditor
:isEditor rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/rodrigop/ontologies/2021/tpc5#address
:address rdf:type owl:DatatypeProperty .


